# Configuração do Banco de Dados PostgreSQL
DB_HOST=
DB_PORT=
DB_USER=
DB_PASSWORD=
DB_NAME=
DB_SSLMODE=  # Define se o SSL será ativado (disable para desenvolvimento)


# Configuração do Redis
REDIS_URL=
# REDIS_URL=localhost:6379 para fora do docker
REDIS_PASSWORD=r
CACHE_TTL_MINUTES=

# Configuração do JWT
JWT_SECRET=  # Chave secreta para assinar tokens JWT
